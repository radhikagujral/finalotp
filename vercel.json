{
    "version": 2,
    "builds": [
        {
            "src":"finalotp/wsgi.py",
            "use":"@vercel/python",
            "config": { "maxLambdaSize": "15mb", "runtime": "python3.9"}

        },
        {
            "src":"buildfiles.sh",
            "use":"@vercel/static-build",
            "config":{"distDir":"staticfiles"}
        }
    ],

    "routes":
    [
        {
        "src": "/static/(.*)",
        "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest":"finalotp/wsgi.py"
        }

    ]

}